{
  "name": "typeguard-generator",
  "version": "0.0.1",
  "description": "For generating typescript typeguards",
  "main": "./out/extension",
  "scripts": {
    "test": "jest",
    "vscode:prepublish": "npm run compile",
    "build": "tsc",
    "compile": "tsc -p ./",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "fix": "eslint '*/**/*.{js,ts,tsx} --fix'",
    "pretest": "npm run build",
    "watch": "tsc -watch -p ./"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinNedergardPrivate/typeguard-generator"
  },
  "keywords": [
    "typeguard"
  ],
  "activationEvents": [
    "onCommand:extension.generateTypeguard"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.generateTypeguard",
        "title": "Generating Typeguards"
      }
    ]
  },
  "author": "Robin Nederg√•rd",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-prettierx": "^0.18.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.1.0",
    "prettierx": "^0.19.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5",
    "watch": "^1.0.2"
  }
}
